<!-- boilerplate for every single ejs file -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <title>Chef's Canvas</title>
</head>
<body class="d-flex flex-column vh-100">
    <%- include("../partials/navbar") %>
    <!-- <h1>BEFORE</h1> -->
    
    <!-- everything goes in between the Before and After h1's, on the file just call layout(directory of boilerplate) -->
    <main class="container mt-5">
        <%- body %>
    </main>
    <!-- <h1>AFTER</h1> -->

    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha384-KyZXEAg3QhqLMpG8r+Knujsl5+5hb7ie3bbZ8eRJ5/SoNUH8N1l5fZq3B+2jNnHq" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.2/dist/umd/popper.min.js" integrity="sha384-7/zNp1ttT1Giyux+emBQRKfn4dEY9ojw9+QeIba6jHHmyPl/jOdO19EHRM8bXr8i" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-QWtKOu1sIU5z39r5E/bK9Yr4U6iM9wDq6Tk4Le4h7A0y7c3/qf/tDdpeQJbMgn7A" crossorigin="anonymous"></script>
</body>
</html>
